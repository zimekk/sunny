{
  "name": "sunny",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || ^22.0.0 || ^24.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{json,js,jsx,md,scss,ts,tsx,css,yml,yaml}": "prettier --write"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "clean": "rm -rf .next",
    "start": "NEXT_TELEMETRY_DISABLED=1 next start",
    "lint": "next lint",
    "reset": "pnpm store prune && rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "update:browserlist": "pnpm update caniuse-lite -r",
    "update:dedupe": "pnpm dedupe",
    "update:latest": "pnpm update --interactive --latest",
    "prepare": "husky && next telemetry disable"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^3.0.0",
    "date-fns": "^4.1.0",
    "next": "16.0.3",
    "observable-hooks": "^4.2.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rxjs": "^7.8.1",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/dom-screen-wake-lock": "^1.0.3",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.14.0",
    "eslint-config-next": "^16.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^16.0.0",
    "prettier": "^3.3.3",
    "type-coverage": "^2.29.7",
    "typescript": "^5.6.3"
  }
}
